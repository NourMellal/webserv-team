{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Parser CGI Processing",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/config_tester",
            "args": ["${workspaceFolder}/config/test.conf"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build config_tester",
            "miDebuggerPath": "/usr/bin/gdb",
            "symbolLoadInfo": {
                "loadAll": true,
                "exceptionList": ""
            }
        }
    ]
}